@using ASP.Net_MVC.Entities
@model List<Employee>



<html>
    <body>
        <h1> Employee List</h1>
        <s></s>
        <table class="table table-bordered table-striped table-hover">
            <thead>
                <tr>
                    <th>EmployeeID</th>
                    <th>FirstName</th>
                    <th>LastName</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
            {
                <tr>
                    <th>@item.EmployeeID</th>
                    <th>@item.FirstName</th>
                    <th>@item.LastName</th>
                </tr>
            }
        </tbody>
        </table>
    </body>
</html>
